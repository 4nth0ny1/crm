<p id="notice"><%= notice %></p>

<div class="head-container">
  <h1>Users</h1>
  <%= link_to 'New User', new_user_path %>
</div>

<table class="table">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.first_name, user  %></td>
        <td><%= link_to user.last_name, user  %></td>
        <td><%= link_to user.email, user %></td>
        <td><%= link_to user.role, user  %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


