<p id="notice"><%= notice %></p>

<div class="head-container">
  <h1>Suggestions</h1>
  <%= link_to 'Create Suggestion', new_suggestion_path, class:"btn btn-primary" %>
</div>

<table class="table">
  <thead>
    <tr>
      <th>Date</th>
      <th>Title</th>
      <th>Description</th>
     
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @suggestions.each do |suggestion| %>
      <tr>
        <td><%= suggestion.created_at.strftime("%m/%d/%Y")%></td>
        <td><%= suggestion.title %></td>
        <td><%= suggestion.description %></td>
        <td><%= link_to "Show", suggestion %></td>
       

      </tr>
    <% end %>
  </tbody>
</table>

<br>


