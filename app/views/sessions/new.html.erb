<p id=”alert”><%= alert %></p>

<h1>Login</h1>

<%= link_to "Sign in with Google", "/auth/google_oauth2", method: :post %>

<%= form_tag sessions_path do |form| %>
  <div class=”field”>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>
  <div class=”field”>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div class=”actions”>
    <%= submit_tag "Login" %>
  </div>
<% end %>